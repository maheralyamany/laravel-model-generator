{
  "name": "maheralyamany/laravel-model-generator",
  "description": "Generate Laravel models from database tables with relationships",
  "keywords": [
    "Maher Alyamany",
    "MaherAlyamany",
    "maheralyamany",
    "laravel",
    "model generator"
  ],
  "support": {
    "issues": "https://github.com/maheralyamany/laravel-model-generator/issues",
    "source": "https://github.com/maheralyamany/laravel-model-generator"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Maher Alyamany",
      "email": "maheralyamany4@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "laravel/framework": ">=10.10",
    "doctrine/dbal": ">=3.1.5",
    "illuminate/support": ">=10.0",
    "illuminate/database": ">=10.0",
    "illuminate/contracts": ">=10.0",
    "illuminate/filesystem": ">=10.0",
    "illuminate/console": ">=10.0",
    "illuminate/config": ">=10.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "mockery/mockery": ">=1.4",
    "phpunit/phpunit": "^10.5.58"
  },
  "autoload": {
    "psr-4": {
      "ModelGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ModelGenerator\\ModelGeneratorServiceProvider"
      ]
    }
  }
}