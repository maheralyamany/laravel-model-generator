{
  "name": "maheralyamany/laravel-model-generator",
  "description": "Generate Laravel models from database tables with relationships",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "MaherAlyamany\\ModelGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MaherAlyamany\\Tests\\Integration\\": "tests/integration"
    }
  },
  "authors": [
    {
      "name": "Maher Alyamany",
      "email": "maheralyamany4@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    
    "illuminate/database": "^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/config": "^10.0|^11.0",
    "illuminate/console": "^10.0|^11.0",
    "doctrine/dbal": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5.58"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MaherAlyamany\\ModelGenerator\\ModelGeneratorServiceProvider"
      ]
    }
  }
}